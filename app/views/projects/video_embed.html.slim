#video_embed
  - if @project.video_url.empty?
    iframe src="#{@project.flickr_square}" width="150" height="150"
  - else
    iframe src="#{@project.vimeo.embed_url}?title=0&amp;byline=0&amp;portrait=0&amp" width="364" height="200"
  end
  h1= link_to @project.name, project_path(@project), :target => '__blank'
  #video_embed_footer
    h2 #{t('.a_project_by_html', :link => link_to(@project.user.display_name, user_path(@project.user), :target => '__blank'))}
    #platform_link
      = link_to root_path, :target => '__blank' do
        = image_tag "logo.png", :title => t('site.name')
